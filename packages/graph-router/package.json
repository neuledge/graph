{
  "name": "@neuledge/graph-router",
  "version": "0.3.1",
  "description": "Router for pattern matching and resolution of queries against data source registries.",
  "keywords": [
    "ai",
    "llm",
    "agents",
    "router",
    "routing",
    "pattern-matching",
    "query-resolution",
    "data-sources",
    "registry",
    "ai-integration",
    "ai-tools",
    "neuledge",
    "graph",
    "function-calling",
    "openai",
    "anthropic",
    "vercel-ai",
    "langchain"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf --glob dist/** && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "npx biome ci --error-on-warnings",
    "fix": "npx biome check --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Neuledge",
  "license": "Apache-2.0",
  "devDependencies": {
    "@neuledge/graph": "workspace:*",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^24.9.1",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3"
  },
  "tsx": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
