    "@openai/agents": "^0.3.7",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.9.1",
    "ai": "^6.0.5",
    "dotenv": "^17.2.3",
    "js-yaml": "^4.1.1",
    "langchain": "^1.2.3",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.24.6"
