{
  "name": "@neuledge/graph",
  "version": "0.2.0",
  "description": "Give LLMs real knowledge — structured, live, and reliable — so they reason with facts, not guesses.",
  "keywords": [
    "ai",
    "llm",
    "agents",
    "openai",
    "anthropic",
    "langchain",
    "vercel-ai",
    "ai-sdk",
    "tool",
    "function-calling",
    "structured-data",
    "live-data",
    "knowledge-graph",
    "real-time-data",
    "data-fetching",
    "ai-integration",
    "ai-tools",
    "lookup",
    "grounding",
    "no-rag",
    "no-indexes",
    "json-api",
    "data-layer",
    "ai-infrastructure",
    "neuledge",
    "weather",
    "stocks",
    "price",
    "fx",
    "rates"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/",
    "bin/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "bin": "./bin/cli.js",
  "scripts": {
    "build": "rimraf --glob dist/** && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "evals": "vitest run --config vitest.config.evals.ts",
    "lint": "npx biome ci --error-on-warnings",
    "fix": "npx biome check --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Neuledge",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/openai": "^3.0.2",
